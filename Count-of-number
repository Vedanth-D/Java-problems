import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter the numbers: ");
        int n = scan.nextInt();
        
        int count = 0;
        int temp = n;
        
        while (temp != 0){
            temp /= 10;
            count++;
        }
        System.out.println("Count of number: " + count);
        scan.close();
    }
}
