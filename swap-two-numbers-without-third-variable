public class Main {
    public static void main(String[] args) {
        int a = 15;
        int b = 24;
        System.out.println("Before swap: " + "a:" + a + " b:" + b);
        
        a = a + b;
        b = a - b;
        a = a - b;
        System.out.println("After swapped: " + "a:" + a + " b:" + b);
    }
}
