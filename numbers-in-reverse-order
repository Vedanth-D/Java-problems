import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter the numer to reverse: ");
       int n = scan.nextInt();
       
       for(int i = n; i >= 0; i--){
           System.out.println(i + " ");
       }
       scan.close();
    }
}
