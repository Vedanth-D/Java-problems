public class Main {
    public static void main(String[] args) {
        int a = 15;
        int b = 24;
        int temp = a;
        System.out.println("Before swap: " + "a:" + a + " b:" + b);
        
        a = b;
        b = temp;
        
        System.out.println("After swapped: " + "a:" + a + " b:" + b);
    }
}
