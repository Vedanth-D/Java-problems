import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter the numbers: ");
        int n = scan.nextInt();
        
        int sum = 0;
        int temp = n;
        
        while (temp != 0){
            int Digit = temp % 10;
            sum += Digit;
            temp /= 10;
        }
        System.out.println("Sum of Digit: " + sum);
        scan.close();
    }
}
